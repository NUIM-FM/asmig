package atongmu.smt;
import atongmu.ast.Vars;

public class Z3Solver implements SMTSolver{
	private Solver solver;
	public Z3Solver(){
		solver=Solver.Z3;
	}

	static{
		System.loadLibrary("Z3Solver");
	}

	public native void solve(String s);
	public String toString(){return solver.toString();}
	
}
