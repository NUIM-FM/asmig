package atongmu.interpreter;

public enum Color{

	WHITE{public String toString(){return "white";}},
	BLACK{public String toString(){return "black";}},
	GRAY75{public String toString(){return "gray75";}},
	RED{public String toString(){return "red";}},
	BLUE{public String toString(){return "skyblue";}},
	GREEN{public String toString(){return "green";}},
	YELLOW{public String toString(){return "yellow";}},
	ORANGE{public String toString(){return "orange";}};
	
}
