import atongmu.ast.*;
import atongmu.atg.*;
import atongmu.type.*;
import atongmu.util.*;
import atongmu.value.*;

public class test{

	public static void main (String args[]){
		Argument arg1 = new Argument(new VarType("x1"),"arg1");
		Argument arg2 = new Argument(new IntType(),"arg2");
		Argument arg3 = new Argument(new VarType("x2"),"arg3");
		BinaryFun b1 = new BinaryFun(Connective.AND,arg1,arg2);
		BinaryFun b2 = new BinaryFun(Connective.OR,b1,arg3);
		System.out.println(b2);
	}
}
