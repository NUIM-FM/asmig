package atongmu.ocl.type;
import atongmu.atg.Edge;
import java.util.TreeSet;

public class RefType extends OCLType{
	public RefType(TreeSet<Edge> edges){super.collection=edges;}
	public TreeSet<Edge> type(){return super.collection;}
	public boolean isRefType(){return true;}
	
}
